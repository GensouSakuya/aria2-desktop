<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:GensouSakuya.Aria2.Desktop.Shell.ViewModels.Other"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="100"
        x:Class="GensouSakuya.Aria2.Desktop.Shell.Views.Other.ErrorDialog"
        Title="ErrorDialog">
  <Window.DataContext>
    <vm:ErrorDialogViewModel></vm:ErrorDialogViewModel>
  </Window.DataContext>
  <TextBlock Text="{Binding Message}"></TextBlock>
</Window>
